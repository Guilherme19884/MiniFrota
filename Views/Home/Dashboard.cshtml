@model MiniFrota.Models.Dashboard

@{
    ViewData["Title"] = "Dashboard";
}
<h1>@ViewData["Title"]</h1>

<div class="container">
    <div class="row">
        <div class="col-md-6">
            <h2>Total</h2>
            <p>@Model.Total</p>
        </div>
        <div class="col-md-6">
            <h2>Total de Emplacamento</h2>
            <p>@Model.TotalEmplacamento</p>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <h2>Total de Abastecimento no Ano</h2>
            <p>@Model.TotalAbastecimentoAno</p>
        </div>
        <div class="col-md-6">
            <h2>Total de Seguro</h2>
            <p>@Model.TotalSeguro</p>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <h2>Multa no Ano</h2>
            <p>@Model.MultaAno</p>
        </div>
        <div class="col-md-6">
            <h2>Total de Manutenção</h2>
            <p>@Model.TotalManutencao</p>
        </div>
    </div>
</div>
